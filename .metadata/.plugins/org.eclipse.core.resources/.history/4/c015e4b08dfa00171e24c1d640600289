package exam;

public class VendingMachine {

	public static void main(String[] args) {
		Drink[] drinks = new Drink[8];
		drinks[0] = new Drink("a",10);
		drinks[0] = new Drink("b",15);
		drinks[0] = new Drink("c",10);
		drinks[0] = new Drink("d",18);
		drinks[0] = new Drink("e",10);
		drinks[0] = new Drink("f",20);
		drinks[0] = new Drink("g",15);
		drinks[0] = new Drink("h",18);
	}

}
